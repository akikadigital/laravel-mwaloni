{
    "name": "akika/laravel-mwaloni",
    "description": "A single-tenant Laravel package for integrating Mpesa, Equity Jenga API,s and NCBA Loop API through Mwaloni Wallet",
    "homepage": "https://github.com/akikadigital/laravel-mwaloni",
    "type": "library",
    "keywords": [
        "mwaloni",
        "wallet",
        "laravel",
        "mpesa",
        "jenga",
        "ncba",
        "loop",
        "equity bank",
        "Akika",
        "Digital"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/support": "^8.0 | ^9.0 | ^10.0 | ^11.0",
		"illuminate/http":"^8.0 | ^9.0 | ^10.0 | ^11.0",
		"guzzlehttp/guzzle": "^7.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Akika\\LaravelMwaloni\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Felix Nyabwari",
            "email": "fenicfelix@gmail.com"
        }
    ],
    "extra": {
		"laravel": {
			"providers": [
				"Akika\\LaravelMwaloni\\MwaloniServiceProvider"
			],
			"aliases": {
				"Mpesa": "Akika\\LaravelMwaloni\\Facades\\Mpesa"
			}
		}
  	}
}
